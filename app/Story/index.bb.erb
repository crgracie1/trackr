<div id="pageTitle">
	<h1>Stories (<%=@iteration%>)</h1>
</div>

<div id="content">
  <table>
    <tr>
    <% @stories.each do |obj| %>
    	
    	    <td class="recordLabel"><%= link_to "#{obj.name}", :action => :show, :query => {:id => obj.object} %></td>
    	
    <% end %>
    </tr>
  </table>
</div>