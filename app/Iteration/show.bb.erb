<div id="pageTitle">
	<h1><%= @iteration.id%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @iteration.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @iteration.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Number</td>
        <td class="itemValue"><%= @iteration.number%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Start</td>
        <td class="itemValue"><%= @iteration.start%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Finish</td>
        <td class="itemValue"><%= @iteration.finish%></td>
      </tr>
      
    </table>
</div>